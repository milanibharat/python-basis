   "source": [
    "#To know how many days left on user's birthday\n",
    "import datetime\n",
    "currentDate=datetime.date.today()\n",
    "User=input(\"Enter your Birtday(DD/MM/YYYY):\")\n",
    "birthdate=datetime.datetime.strptime(User,'%d/%m/%Y').date()\n",
    "print(birthdate)\n",
    "days=birthdate-currentDate\n",
    "print(days)"
   ]
  }
